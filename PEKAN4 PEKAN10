<!DOCTYPE html>
<html>
<head>
    <title>Slideshow</title>
    <style>
        #slide {
            width: 400px;
            height: 250px;
            position: relative;
            overflow: hidden;
        }

        #slide img {
            width: 100%;
            height: 100%;
            position: absolute;
            opacity: 0;
            transition: 0.5s;
            object-fit: cover;
        }

        #slide img.active {
            opacity: 1;
        }
    </style>
</head>
<body>

<h2>Slideshow Gambar</h2>

<div id="slide">
    <img src="data:image/webp;base64," class="active">
    <img src="data:image/webp;base64,">
    <img src="https://th.bing.com/th/id/OIP.dtk2Eeq8YK3kaGXF-DRPgAHaED?w=301&h=180&c=7&r=0&o=7&cb=ucfimg2&pid=1.7&rm=3&ucfimg=1">
</div>

<br>
<button onclick="prev()">Prev</button>
<button onclick="next()">Next</button>

<script>
    let index = 0;
    const imgList = document.querySelectorAll("#slide img");

    function show(i){
        imgList.forEach(img => img.classList.remove("active"));
        imgList[i].classList.add("active");
    }

    function next(){
        index = (index + 1) % imgList.length;
        show(index);
    }

    function prev(){
        index = (index - 1 + imgList.length) % imgList.length;
        show(index);
    }
</script>

</body>
</html>
